<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddFoodController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="193.0" text="ID" />
          <TableColumn fx:id="nameColumn" prefWidth="200.0" text="Food" />
          <TableColumn fx:id="caloriesColumn" prefWidth="200.0" text="Calories (cal)" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#handleButtonClick" style="-fx-background-color: green; -fx-text-fill: white;" text="Add Foods" />
      <ChoiceBox fx:id="mealtimeChoiceBox" prefWidth="150.0" />
   </children>
</VBox>
